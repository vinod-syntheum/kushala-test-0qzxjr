{
  "name": "restaurant-platform-backend",
  "version": "1.0.0",
  "description": "Backend service for the Digital Presence MVP for Small Restaurants platform",
  "main": "dist/server.js",
  "scripts": {
    "start": "node dist/server.js",
    "dev": "nodemon --watch 'src/**/*.ts' --exec 'ts-node' src/server.ts",
    "build": "tsc",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --write 'src/**/*.{ts,json}'",
    "typeorm": "typeorm-ts-node-commonjs",
    "migrate:generate": "npm run typeorm migration:generate -- -n",
    "migrate:run": "npm run typeorm migration:run -- -d src/db/data-source.ts",
    "migrate:revert": "npm run typeorm migration:revert -- -d src/db/data-source.ts",
    "seed:run": "ts-node ./src/db/seeds/run-seeds.ts",
    "security:audit": "npm audit",
    "clean": "rimraf dist coverage",
    "prepare": "husky install"
  },
  "dependencies": {
    "@aws-sdk/client-kms": "^3.0.0",
    "@aws-sdk/client-s3": "^3.0.0",
    "@aws-sdk/middleware-retry": "^3.0.0",
    "@googlemaps/google-maps-services-js": "^3.3.0",
    "@nestjs/cache-manager": "^2.0.0",
    "@nestjs/common": "^10.0.0",
    "@nestjs/swagger": "^7.0.0",
    "@nestjs/throttler": "^5.0.0",
    "@sendgrid/mail": "^7.7.0",
    "@sentry/node": "7.x",
    "argon2": "^0.30.0",
    "bottleneck": "^2.19.5",
    "bull": "^4.12.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dayjs": "^1.11.0",
    "express": "^4.18.0",
    "express-rate-limit": "^6.7.0",
    "geojson": "^2.0.0",
    "helmet": "^7.0.0",
    "ioredis": "^5.3.0",
    "jsonwebtoken": "^9.0.0",
    "knex": "^2.5.0",
    "mongodb": "^5.0.0",
    "mongoose": "^6.0.0",
    "morgan": "^1.10.0",
    "ms": "^2.1.3",
    "newrelic": "10.x",
    "opossum": "^6.0.0",
    "rate-limiter-flexible": "^2.4.1",
    "redis": "^7.0.0",
    "retry": "^0.13.0",
    "stripe": "^12.0.0",
    "tsyringe": "^4.7.0",
    "typeorm": "^0.3.0",
    "uuid": "^9.0.0",
    "winston": "^3.8.0",
    "winston-daily-rotate-file": "^4.7.1",
    "zod": "^3.0.0"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.0.0",
    "@jest/globals": "^29.0.0",
    "@jest/types": "^29.0.0",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.0",
    "@types/node": "^18.0.0",
    "@types/supertest": "^6.0.0",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.38.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "husky": "^8.0.0",
    "jest": "^29.0.0",
    "lint-staged": "^13.2.1",
    "mongodb-memory-server": "^8.0.0",
    "nodemon": "^2.0.22",
    "prettier": "^2.8.0",
    "redis-mock": "^0.56.3",
    "rimraf": "^5.0.0",
    "supertest": "^6.0.0",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "lint-staged": {
    "*.ts": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/organization/restaurant-platform.git"
  }
}