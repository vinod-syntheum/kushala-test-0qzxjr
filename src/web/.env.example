# WARNING: Do not commit actual .env file to version control

# API Configuration
# Format: http(s)://<host>:<port>/api/<version>
# Example: http://localhost:3000/api/v1 for development
NEXT_PUBLIC_API_URL=http://localhost:3000/api/v1

# Stripe Configuration
# Format: pk_test_* for test, pk_live_* for production
# Obtain from Stripe Dashboard: https://dashboard.stripe.com/apikeys
NEXT_PUBLIC_STRIPE_KEY=pk_test_your_stripe_public_key

# Google Maps Configuration
# Format: AIza... (40 characters)
# Obtain from Google Cloud Console: https://console.cloud.google.com
NEXT_PUBLIC_GOOGLE_MAPS_KEY=your_google_maps_api_key

# Environment Configuration
# Valid values: development, staging, production
NEXT_PUBLIC_ENVIRONMENT=development

# Version Information
# Format: semantic versioning (major.minor.patch)
NEXT_PUBLIC_APP_VERSION=1.0.0

# Support Contact
# Format: valid email address for technical support
NEXT_PUBLIC_SUPPORT_EMAIL=support@example.com